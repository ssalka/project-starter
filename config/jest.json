{
  "globals": {
    "__DEV__": true,
    "ts-jest": {
      "skipBabel": true
    }
  },
  "rootDir": "../",
  "moduleFileExtensions": [
    "js",
    "ts",
    "tsx"
  ],
  "moduleDirectories": [
    "node_modules",
    "."
  ],
  "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js",
  "testPathIgnorePatterns": [
    "/node_modules/"
  ],
  "testRegex": "/test/.*\\.spec\\.tsx?$",
  "testURL": "http://localhost/",
  "transform": {
    ".tsx?": "ts-jest"
  },
  "verbose": true
}
