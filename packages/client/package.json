{
  "name": "client",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "add-component": "pnpm dlx shadcn@latest add",
    "analyze:build": "ANALYZE=1 pnpm build",
    "build": "vite build",
    "dev": "vite",
    "format": "prettier \"src/**/*.{ts,tsx}\" \"e2e/**/*.{ts,tsx}\" --write",
    "format:check": "prettier \"src/**/*.{ts,tsx}\" \"e2e/**/*.{ts,tsx}\" --check",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint --format codeframe . ",
    "gen:routes": "tsr generate",
    "start": "vite preview",
    "test": "cross-env VITE_USER_NODE_ENV=test vitest",
    "test:ui": "cross-env VITE_USER_NODE_ENV=test vitest --ui",
    "test:e2e": "cross-env VITE_E2E=1 NODE_ENV=test playwright test",
    "test:e2e:ui": "cross-env VITE_E2E=1 NODE_ENV=test playwright test --ui",
    "typecheck": "tsgo -b",
    "typecheck:track": "tsx ../scripts/src/typecheck"
  },
  "dependencies": {
    "@ssalka/common": "workspace:*",
    "@ssalka/ui": "workspace:*",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-router": "^1.161.1",
    "@trpc/client": "^11.10.0",
    "@trpc/tanstack-react-query": "^11.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "immer": "^11.1.4",
    "lodash-es": "^4.17.23",
    "lucide-react": "^0.574.0",
    "posthog-js": "^1.351.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "superjson": "^2.2.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@tailwindcss/vite": "^4.2.0",
    "@tanstack/react-router-devtools": "^1.161.1",
    "@tanstack/router-cli": "^1.161.1",
    "@tanstack/router-plugin": "^1.161.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "happy-dom": "^20.6.3",
    "msw": "^2.12.10",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwind-merge": "^3.5.0",
    "tailwindcss": "^4.2.0",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.21.0",
    "vite": "^7.3.1",
    "vite-bundle-analyzer": "^1.3.6",
    "vite-plugin-static-copy": "^3.2.0",
    "vite-tsconfig-paths": "^6.1.1"
  }
}
