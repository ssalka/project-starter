{
  "name": "client",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "add-component": "pnpm dlx shadcn@latest add",
    "analyze:build": "ANALYZE=1 pnpm build",
    "build": "vite build",
    "dev": "vite",
    "format": "prettier \"src/**/*.{ts,tsx}\" \"e2e/**/*.{ts,tsx}\" --write",
    "format:check": "prettier \"src/**/*.{ts,tsx}\" \"e2e/**/*.{ts,tsx}\" --check",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint --format codeframe . ",
    "gen:routes": "tsr generate",
    "start": "vite preview",
    "test": "cross-env VITE_USER_NODE_ENV=test vitest",
    "test:ui": "cross-env VITE_USER_NODE_ENV=test vitest --ui",
    "test:e2e": "cross-env VITE_E2E=1 NODE_ENV=test playwright test",
    "test:e2e:ui": "cross-env VITE_E2E=1 NODE_ENV=test playwright test --ui",
    "typecheck": "tsgo -b",
    "typecheck:track": "tsx ../scripts/src/typecheck"
  },
  "dependencies": {
    "@ssalka/common": "workspace:*",
    "@ssalka/ui": "workspace:*",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-router": "^1.143.3",
    "@trpc/client": "^11.6.0",
    "@trpc/tanstack-react-query": "^11.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "immer": "^10.1.3",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.546.0",
    "posthog-js": "^1.279.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "superjson": "^2.2.6",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/vite": "^4.1.15",
    "@tanstack/react-router-devtools": "^1.143.4",
    "@tanstack/router-cli": "^1.143.3",
    "@tanstack/router-plugin": "^1.143.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.0",
    "happy-dom": "^20.0.8",
    "msw": "^2.11.6",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.15",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "vite": "^7.1.11",
    "vite-bundle-analyzer": "^1.3.6",
    "vite-plugin-static-copy": "^3.1.4",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
